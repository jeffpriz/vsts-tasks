{
	"manifestVersion": 1,
	"id": "vnzk-vsts-tasks-for-tfs",
	"name": "VSTS Create Release Task",
	"version": "0.1.13",
	"publisher": "OneLuckiDev",
	"public": false,
	"targets": [
		{
			"id": "Microsoft.VisualStudio.Services"
		}
	],
	"scope": [
		"vso.build",
		"vso.release"
	],
	"description": "Creates new release as part of current release pipe line.",
	"categories": [
		"Build and release"
	],
	"tags": [
		"build",
		"release",
		"trigger",
        "tasks",
        "linked",
        "create"
	],
	"content": {
		"details": {
			"path": "README.md"
		},
		"license": {
			"path": "LICENSE",
			"contentType": "text/plain"
		}
	},
	"links": {
		"getstarted": {
			"uri": "https://github.com/Venzhyk/vsts-tasks"
		},
		"support": {
			"uri": "https://github.com/Venzhyk/vsts-tasks/issues"
		}
	},
	"branding": {
		"color": "#eff1f3",
		"theme": "light"
	},
	"icons": {
		"default": "imgs/logo.png"
	},
	"files": [
		{
			"path": "Tasks/CreateRelease/"
		}
	],
	"contributions": [
		{
			"id": "vnzk.Tasks.CreateRelease",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "Tasks/CreateRelease"
			}
		}
	]
}